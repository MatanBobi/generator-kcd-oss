{
  "name": "generator-kcd-oss",
  "version": "0.0.0-semantically-released",
  "description": "A generator for my projects. Feel free to contribute if you want to adapt it for more use cases",
  "scripts": {
    "lint": "kcd-scripts lint",
    "validate": "kcd-scripts validate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "kcd-scripts pre-commit"
    }
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "open source",
    "kentcdodds",
    "yeoman-generator"
  ],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "dependencies": {
    "lodash.kebabcase": "^4.1.1",
    "npm-check": "^5.5.2",
    "yeoman-generator": "^4.0.0"
  },
  "devDependencies": {
    "kcd-scripts": "^1.4.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/generator-kcd-oss.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/generator-kcd-oss/issues"
  },
  "homepage": "https://github.com/kentcdodds/generator-kcd-oss#readme"
}
