{
  "name": "generator-kcd-oss",
  "version": "0.0.0-semantically-released",
  "description": "A generator for my projects. Feel free to contribute if you want to adapt it for more use cases",
  "scripts": {
    "start": "nps",
    "test": "nps lint",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "opt --in pre-commit --exec \"npm start validate\""
  },
  "files": [
    "generators",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "open source",
    "kentcdodds",
    "yeoman-generator"
  ],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "dependencies": {
    "lodash.kebabcase": "^4.1.1",
    "npm-check": "^5.4.0",
    "yeoman-generator": "^0.24.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^3.0.7",
    "commitizen": "^2.8.1",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^3.10.2",
    "eslint-config-kentcdodds": "^11.1.0",
    "husky": "^0.11.7",
    "opt-cli": "^1.4.2",
    "p-s": "^3.0.3",
    "rimraf": "^2.5.2",
    "semantic-release": "^6.3.2",
    "validate-commit-msg": "^2.6.1"
  },
  "eslintConfig": {
    "extends": "kentcdodds"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/generator-kcd-oss.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/generator-kcd-oss/issues"
  },
  "homepage": "https://github.com/kentcdodds/generator-kcd-oss#readme"
}
